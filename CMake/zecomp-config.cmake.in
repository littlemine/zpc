set(__supported_components zpc zpctool zecomp)
foreach(_comp ${Zensim_FIND_COMPONENTS})
  if (NOT _comp IN_LIST _supported_components)
    set(Zensim_FOUND False)
    set(Zensim_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
  endif()
endforeach()
